{
  "name": "roakov",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">= 12.2.0"
  },
  "license": "MIT",
  "scripts": {
    "start": "tsc",
    "roakov": "node --loader ts-node/esm/transpile-only ./src/cli.ts",
    "rm:model": "node -e 'fs.rmSync(`./src/assets/model.json`, {recursive:true, force:true})'",
    "prebuild": "node -e 'fs.rmSync(`lib`, {recursive:true, force:true})'",
    "build": "ncc build src/index.ts --out lib --minify"
  },
  "devDependencies": {
    "@types/node": "16.11.6",
    "eslint": "8.1.0",
    "eslint-config-sumikko": "1.2.2",
    "prettier": "2.4.1",
    "ts-node": "10.4.0",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "kuromojin": "3.0.0",
    "meow": "10.1.1"
  }
}
